#include "qca955x.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "mikrotik,routerboard-groovega-52HPacn", "qca,qca9556";
	model = "MikroTik RouterBOARD GrooveGA-52HPacn";

	aliases {
		mdio-gpio1 = &mdio2;
		serial0 = &uart;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 1 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};
	leds: leds {
		compatible = "gpio-leds";
		
		// power {
		// 	label = "blue:power";
		// 	gpios = <&gpio 17 GPIO_ACTIVE_HIGH>;
		// 	default-state = "on";
		// };

		// led_user: user {
		// 	label = "green:user";
		// 	gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		// };

		lan {
			label = "green:lan";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};

		// wlan {
		// 	label = "green:wlan";
		// 	gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
		// 	linux,default-trigger = "phy0tpt";
		// };

		rssilow {
			label = "green:rssilow";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};

		rssimediumlow {
			label = "green:rssimediumlow";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
		};

		rssimedium {
			label = "green:rssimedium";
			gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
		};

		rssimediumhigh {
			label = "green:rssimediumhigh";
			gpios = <&gpio 20 GPIO_ACTIVE_LOW>;
		};

		rssihigh {
			label = "green:rssihigh";
			gpios = <&gpio 21 GPIO_ACTIVE_LOW>;
		};
	};

	mdio2: mdio {
		compatible = "virtual,mdio-gpio";

		gpios = <&gpio 12 GPIO_ACTIVE_HIGH>, /* MDC */
			<&gpio 11 GPIO_ACTIVE_HIGH>; /* MDIO */
		#address-cells = <1>;
		#size-cells = <0>;

		phy0: ethernet-phy@0 {
			reg = <0>;
			device_type = "ethernet-phy";

			phy-mode = "sgmii";

			at803x-override-sgmii-link-check;
		};
		
	};
};

&eth1 {
	status = "okay";

	pll-data = <0x03000101 0x80000101 0x80001313>;
	phy-handle = <&phy0>;

	qca955x-sgmii-fixup;

	gmac-config {
		device = <&gmac>;
	};
};

&spi {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "RouterBoot";
				reg = <0x0 0x20000>;
				compatible = "mikrotik,routerboot-partitions";
				#address-cells = <1>;
				#size-cells = <1>;

				partition@0 {
					label = "bootloader1";
					reg = <0x0 0x0>;
					read-only;
				};

				hard_config: hard_config {
					label = "hard_config";
					read-only;
				};

				bios {
					label = "bios";
					size = <0x1000>;
					read-only;
				};

				partition@10000 {
					label = "bootloader2";
					reg = <0x10000 0x0>;
					read-only;
				};

				soft_config {
					label = "soft_config";
					
				};
			};

			partition@20000 {
				compatible = "mikrotik,minor";
				label = "firmware";
				reg = <0x020000 0xfe0000>;
			};
		};
	};
};

&wmac {
	status = "okay";

	qca,no-eeprom;
};

&pcie0 {
	status = "okay";

	wifi@0,0 {
		compatible = "qcom,ath10k";
		reg = <0 0 0 0 0>;
	};
};